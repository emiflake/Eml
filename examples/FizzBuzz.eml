begin FizzBuzz

divides = \a b. (eq (mod a b) 0)

fizzbuzz =
  let one = \x.
    if (and (divides x 3) (divides x 5)) then
      "Fizzbuzz"
    else if (divides x 3) then
      "Fizz"
    else if (divides x 5) then
      "Buzz"
    else
      (show x)
  in
  \n. if (eq 0 n) then "" else (((fizzbuzz (n - 1)) + "\n") + (one n))

main = \_. (log (fizzbuzz 100))
