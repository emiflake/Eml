begin Types

true = <(eval "true") : Bool>
false = <(eval "false") : Bool>

not = <(eval "x=>!x") : Bool -> Bool>

not' = \x. if x then false else true

a = \x y. (x + y)

test = ((a 2) 3)

concat = <(eval "x=>y=>x+y") : String -> String -> String>

greet = \name. (concat "Hello, " name)

log = <(eval "x=>console.log(x)") : String -> Unit>

identity = <\x. x : Num -> Num>

main = (log "Hello")
